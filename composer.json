{
	"require" : {
		"php" : ">=5.6",
		"psr/log" : "~1.0",
		"ext-sockets" : "*",
		"ext-spl" : "*"
	},
	"require-dev" : {
		"phpunit/phpunit" : "~6.0",
		"phpunit/php-code-coverage" : "~5.0",
		"phpunit/phpunit-mock-objects" : "~4.0",
		"sebastian/resource-operations" : "~1.0",
		"squizlabs/php_codesniffer" : "~2.0",
		"phploc/phploc" : "~3.0",
		"pdepend/pdepend" : "~2.0",
		"phpmd/phpmd" : "~2.0",
		"sebastian/phpcpd" : "~3.0",
		"theseer/phpdox" : "~0.9"
	},
	"name" : "nkey/phpgenerics",
	"description" : "A generic component framework",
	"homepage" : "https://github.com/maikgreubel/phpgenerics",
	"license" : "BSD-3-Clause",
	"authors" : [{
			"name" : "Maik Greubel",
			"email" : "greubel@nkey.de",
			"homepage" : "https://www.nkey.de",
			"role" : "Maintainer"
		}
	],
	"support" : {
		"email" : "greubel@nkey.de",
		"source" : "https://github.com/maikgreubel/phpgenerics",
		"issues" : "https://github.com/maikgreubel/phpgenerics/issues",
		"wiki" : "https://github.com/maikgreubel/phpgenerics/wiki"
	},
	"autoload" : {
		"psr-4" : {
			"Generics\\" : "src/Generics"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"Generics\\Tests\\" : [
				"tests/exception-tests",
				"tests/http-tests",
				"tests/logger-tests",
				"tests/socket-tests",
				"tests/stream-tests",
				"tests/util-tests"
			]
		}
	},
	"config" : {
		"vendor-dir" : "vendor"
	},
	"keywords" : [
		"framework",
		"component"
	],
	"extra" : {
		"branch-alias" : {
			"dev-master" : "1.1-dev"
		}
	},
	"type" : "library"
}