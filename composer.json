{
	"require" : {
		"php" : ">=5.4",
		"psr/log" : "1.0.0"
	},
	"require-dev" : {
		"phpunit/phpunit" : "4.8.6",
		"squizlabs/php_codesniffer" : "2.3.4",
		"phploc/phploc" : "2.1.4",
		"pdepend/pdepend" : "2.1.0",
		"phpmd/phpmd" : "2.2.3",
		"sebastian/phpcpd" : "2.0.2",
		"theseer/phpdox" : "0.8.1.1",
		"satooshi/php-coveralls" : "dev-master"
	},
	"name" : "nkey/phpgenerics",
	"description" : "A generic component framework",
	"homepage" : "https://github.com/maikgreubel/phpgenerics",
	"license" : "BSD-2-Clause",
	"authors" : [{
			"name" : "Maik Greubel",
			"email" : "greubel@nkey.de",
			"homepage" : "https://www.nkey.de",
			"role" : "Maintainer"
		}
	],
	"support" : {
		"email" : "greubel@nkey.de",
		"source" : "https://github.com/maikgreubel/phpgenerics",
		"issues" : "https://github.com/maikgreubel/phpgenerics/issues",
		"wiki" : "https://github.com/maikgreubel/phpgenerics/wiki"
	},
	"autoload" : {
		"psr-4" : {
			"Generics\\" : "src/Generics"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"Generics\\Tests\\" : [
				"tests/exception-tests",
				"tests/http-tests",
				"tests/logger-tests",
				"tests/socket-tests",
				"tests/stream-tests",
				"tests/util-tests"
			]
		}
	},
	"config" : {
		"vendor-dir" : "vendor"
	},
	"keywords" : [
		"framework",
		"component"
	],
	"extra" : {
		"branch-alias" : {
			"dev-master" : "1.1-dev"
		}
	},
	"type" : "library"
}