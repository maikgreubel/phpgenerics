{
	"require-dev" : {
		"phpunit/phpunit" : "4.3.*",
		"phpdocumentor/phpdocumentor" : "2.8.1",
		"phpdocumentor/template-zend" : "1.3.2"
	},
	"name" : "nkey/phpgenerics",
	"description" : "A generic component framework",
	"homepage" : "https://github.com/maikgreubel/phpgenerics",
	"license" : "BSD-2-Clause",
	"minimum-stability" : "dev",
	"authors" : [{
			"name" : "Maik Greubel",
			"email" : "greubel@nkey.de",
			"homepage" : "https://www.nkey.de",
			"role" : "Maintainer"
		}
	],
	"support" : {
		"email" : "greubel@nkey.de",
		"source" : "https://github.com/maikgreubel/phpgenerics",
		"issues" : "https://github.com/maikgreubel/phpgenerics/issues",
		"wiki" : "https://github.com/maikgreubel/phpgenerics/wiki"
	},
	"autoload" : {
		"psr-4" : {
			"Generics" : "Generics",
			"Generics/Client" : "Generics/Client",
			"Generics/Logger" : "Generics/Logger",
			"Generics/Socket" : "Generics/Socket",
			"Generics/Streams" : "Generics/Streams",
			"Psr/Log" : "Psr/Log"
		}
	},
	"config" : {
		"vendor-dir" : "r"
	},
	"keywords" : [
		"framework",
		"component"
	],
	"scripts": {
		"test": [ "phpunit -c ./tests/phpunit.xml" ],
		"documentation": [ "phpdoc" ]
	}
}